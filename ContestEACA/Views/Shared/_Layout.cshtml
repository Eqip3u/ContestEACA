<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ContestEACA</title>

        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/bootstrap-social/bootstrap-social.css" />
        <link rel="stylesheet" href="~/lib/normalize-css/normalize.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.css" />
        <link rel="stylesheet" href="~/lib/fullpage.js/dist/jquery.fullpage.css" />
        <link rel="stylesheet" href="~/css/site.css" />

    <script src="~/lib/jquery/dist/jquery.js"></script>

</head>
<body>

    <nav class="navbar navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand navbar-brand-name">
                    <span>
                        <img class="logo" src="~/Inga/eacu.png" />
                    </span>
                    Конкурсы ЕАСИ
                </a>
            </div>
            <div class="navbar-collapse collapse">

                @await Html.PartialAsync("_TopLinePartial")

                @await Html.PartialAsync("_LoginPartial")

            </div>
        </div>
    </nav>


    <div class="container testdeletediv">

        @RenderBody()

    </div>
              
    @*<footer >
        <div class="container">
            <p> &copy; 2017 - Конкурсы ЕАСИ</p>
        </div>
    </footer>*@
   
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/fullpage.js/dist/jquery.fullpage.min.js"></script>
    <script src="~/lib/bootstrap-autohide-navbar/dist/bootstrap-autohide-navbar.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    @RenderSection("Scripts", required: false)
</body>
</html>
